{
  "name": "fcl-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev & flow emulator start -v --dev-wallet",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "getEvents": "flow scripts execute ./src/cadence/scripts/get_float_event_ids.cdc 0x958526fe01b782a0 --network=testnet",
