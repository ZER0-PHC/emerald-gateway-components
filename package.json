    "@sveltejs/kit": "1.0.0-next.202",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.32",
    "canvas-confetti": "^1.5.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "svelte": "^3.44.3"
